{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/angular-app/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "angular-neutralino",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/angular-app/src/favicon.ico"
    }
  },
  "cli": {
    "binaryName": "angular-neutralino",
    "extensionsPath": "/extensions/",
    "resourcesPath": "/angular-app/dist/",
    "clientLibrary": "/angular-app/src/assets/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0",
    "frontendLibrary": {
      "patchFile": "/angular-app/src/index.html",
      "devUrl": "http://localhost:4200"
  }
  }
}